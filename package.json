{
  "name": "duo-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@esss-swap/duo-localisation": "^1.1.7",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@tinymce/tinymce-react": "^3.3.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-router": "^5.1.3",
    "@types/yup": "^0.26.26",
    "@types/dateformat": "^3.0.1",
    "clsx": "^1.0.4",
    "dateformat": "^3.0.3",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "express-http-proxy": "^1.6.0",
    "formik": "^1.5.8",
    "formik-material-ui": "0.0.18",
    "graphql": "^14.5.8",
    "graphql-request": "^1.8.2",
    "html-react-parser": "^0.9.2",
    "http-proxy-middleware": "^0.20.0",
    "immer": "^4.0.2",
    "jsonwebtoken": "^8.5.1",
    "material-table": "^1.54.2",
    "notistack": "^0.9.7",
    "path": "^0.12.7",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "tinymce": "^5.1.4",
    "typescript": "^3.7.4",
    "yup": "^0.27.0"
  },
  "proxy": "http://localhost:4000",
  "scripts": {
    "predev": "npm run generate:local",
    "dev": "react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "start": "node server.js",
    "cypress:open": "cypress open",
    "cy:run": "cypress run",
    "generate:local": "sed 's@<SCHEMA_URL>@http://localhost:4000/graphql@g' codegen_template.yml > codegen_tmp.yml && graphql-codegen -c codegen_tmp.yml && rm ./codegen_tmp.yml",
    "generate:dev": "sed 's@<SCHEMA_URL>@PLACEHOLDER@g' codegen.yml > codegen_tmp.yml && graphql-codegen -c codegen_tmp.yml && ./rm codegen_tmp.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.11.2",
    "@graphql-codegen/typescript": "^1.11.2",
    "@graphql-codegen/typescript-graphql-request": "^1.11.2",
    "@graphql-codegen/typescript-operations": "^1.11.2",
    "@types/dateformat": "^3.0.1",
    "@types/faker": "^4.1.8",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-test-renderer": "^16.9.1",
    "cypress": "^3.8.1",
    "faker": "^4.1.0",
    "react-test-renderer": "^16.12.0"
  }
}
